<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <TextView
        android:id="@+id/id_first_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama Depan" />


    <TextView
        android:id="@+id/id_last_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama Belakang" />


    <TextView
        android:id="@+id/id_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No Phone" />


    <TextView
        android:id="@+id/id_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email" />

    <TextView
        android:id="@+id/id_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password" />


    <TextView
        android:id="@+id/id_level"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Level" />

    <TextView
        android:id="@+id/id_token"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Token" />

    <TextView
        android:id="@+id/id_last_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="last login" />

    <TextView
        android:id="@+id/id_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="login" />

</LinearLayout>